<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.test1.cardioview">

    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/CardioView"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewmedicine"></activity>
        <activity android:name=".addmedicine" />
        <activity android:name=".addcheckup" />
        <activity android:name=".viewcheckup" />
        <activity android:name=".medicine" />
        <activity android:name=".checkup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".workout" />
        <activity android:name=".heartcare" />
        <activity android:name=".anginapain" />
        <activity android:name=".cardiacarrest" />
        <activity android:name=".activity_heartattack" />
        <activity android:name=".activity_search" />
        <activity
            android:name=".activity_heartbeat"
            tools:ignore="WrongManifestParent" />
        <activity
            android:name=".activitry_chestpain"
            tools:ignore="WrongManifestParent" />
        <activity
            android:name=".activity_know"
            tools:ignore="WrongManifestParent" />
    </application>

</manifest>